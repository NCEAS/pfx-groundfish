  Covar$cent.log.depth	<-	Covar$log.depth - mean(Covar$log.depth,na.rm=T)#
  Covar$cent.log.depth.2	<-	Covar$log.depth^2 #
  Covar$cent.temp			<-	Covar$BottomTemp - mean(Covar$BottomTemp)#
  Covar$cent.temp.2		<-	Covar$cent.temp^2 #
  setwd(myWD)#
